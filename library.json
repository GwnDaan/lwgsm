{
	"name": "LwGSM",
	"version": "0.1.1",
	"description": "Lightweight GSM AT commands parser for embedded systems and SimCOM modules",
	"keywords": "lwgsm, simcom, quectel, lightweiight, AT, AT Commands",
	"repository": {
		"type": "git",
		"url": "https://github.com/MaJerle/lwgsm.git"
	},
	"authors": [
		{
			"name": "Tilen Majerle",
			"email": "tilen@majerle.eu",
			"url": "https://majerle.eu"
		}
	],
	"license": "MIT",
	"homepage": "https://github.com/MaJerle/lwgsm",
	"dependencies": {},
	"frameworks": "*",
	"platforms": "*",
	"export": {
		"exclude": [
			".github",
			"dev",
			"docs",
			"**/.vs",
			"**/Debug",
			"build",
			"**/build"
		]
	},
	"build": {
		"flags": [
			"-Ilwgsm/src/include",
			"-Idev",
			"-Ilwgsm/src/include/system/port/freeRTOS"
		],
		"srcFilter": [
			"+<*>",
			"-<docs/>",
			"-<examples/>",
			"-<snippets/>",
			"-<dev/>",
			"-<lwgsm/src/system>",
			"+<lwgsm/src/system/lwgsm_sys_freeRTOS.c>",
			"+<lwgsm/src/system/lwgsm_ll_esp32.c>"
		]
	}
}